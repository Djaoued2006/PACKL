func strlen(s: str): int {
    var i: int = 0;
    var current: int = 0;
    var notstop: int = 1;
    while(notstop) {
        current = get_byte(s, i);
        if (current) { i = i + 1; }
        else { notstop = 0; }
    }
    strlen = i;
}

proc main() {
    var length: int = strlen("PACKL");
    exit(length);
}