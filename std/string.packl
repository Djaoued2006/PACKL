func len(s: str): int {
    var i: int = 0;
    var current: int = 0;
    var notstop: int = 1;

    while(notstop) {
        current = s[i];
        if (current) { i = i + 1; }
        else { notstop = 0; }
    }
    
    len = i;
}

func sum(s: str): int {
    sum = 0;
    var size: int = len(s);
    for i: int in (0, size - 1) {
        sum = sum + s[i];
    }
}
