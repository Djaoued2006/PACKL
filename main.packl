record P {
    x: int;
};

record Point {
    x: int;
    y: int;
};

func init_point(x: int, y: int): Point {
    var p: Point;
    p.x = x;
    p.y = y;
    init_point = p;
}

proc print_point(p: Point) {
    write(0, "(", p.x, ", ", p.y, ")\n");
}


proc main() {
    var p: Point;
    p = init_point(1, 2);
    print_point(p);
    exit(p.x - 1);
}